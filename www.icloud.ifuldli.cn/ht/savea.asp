<script>
    $(function(){
        $("#aaa").attr("disabled", true);
        $("#btn_commit").attr("disabled", true);
        $("#aaa").removeClass().addClass("login_2");
        $("#btn_commit").removeClass().addClass("login_1");
        $("#q1,#q2,#q3").keyup(function(){
            var question1 = $("#question1 option:selected").val();
            var question2 = $("#question2 option:selected").val();
            var question3 = $("#question3 option:selected").val();
            var q1 = $("#q1").val();
            var q2 = $("#q2").val();
            var q3 = $("#q3").val();
            if(question1 == "" || question2 == "" || question3=="" || $.trim(q1) == "" || $.trim(q2) == "" || $.trim(q3) == "") {
                $("#aaa").attr("disabled", true);
                $("#btn_commit").attr("disabled", true);
                $("#aaa").removeClass().addClass("login_2");
                $("#btn_commit").removeClass().addClass("login_1");
            } else {
                $("#aaa").attr("disabled", false);
                $("#btn_commit").attr("disabled", false);
                $("#aaa").removeClass().addClass("login_1");
                $("#btn_commit").removeClass().addClass("login_2");
            }   
        });
        $("#btn_commit").click(function(){
            var question1 = $("#question1 option:selected").val();
            var question2 = $("#question2 option:selected").val();
            var question3 = $("#question3 option:selected").val();
            var q1val = $("#q1").val();
            var q2val = $("#q2").val();
            var q3val = $("#q3").val();
            if(question1 == "") {
                alert("选择第一个安全问题");
                $("#question1").focus();
                return;
            }           
            
            if(q1val == "") {
                alert("请填入您的问题");
                $("#q1").focus();
                return;
            }
            if(q1val.length < 1) {
                alert("请重新输入您的问题");
                $("#q1").focus();
                return;
            }
            if(question2 == "") {
                alert("选择第二个安全问题");
                $("#question2").focus();
                return;
            }
            if(q2val == "") {
                alert("请填入您的问题不正确");
                $("#q2").focus();
                return;
            }
            if(q2val.length < 1) {
                alert("请重新输入您的问题");
                $("#q2").focus();
                return;
            }
            if(question3 == "") {
                alert("选择第三个安全问题");
                $("#question3").focus();
                return;
            }
            if(q3val == "") {
                alert("请填入您的问题不正确");
                $("#q3").focus();
                return;
            }
            if(q3val.length < 1) {
                alert("请重新输入您的问题");
                $("#q3").focus();
                return;
            }
            var pp = new Object();
			pp.mbq1 = escape(question1);
            pp.mba1 = escape(q1val);
            pp.mbq2 = escape(question2);
			pp.mba2 = escape(q2val);
            pp.mbq3 = escape(question3);
			pp.mba3 = escape(q3val);
       //js
        var url = 'save3.asp';        
        $.ajax({
            url: url,           
            type:'get',
            data: pp,
            async:true,
			dataType: "json",
			success: function (data) {
                    callBack(data);
                }
			//jsonpCallback: "receive",
            //jsonp: "callback",
			//dataType: "jsonp",
            /*success: function(revdata) {
                if(revdata.status == 0) {//代表服务器返回成功
                    alert("请继续下一步！");//弹出信息
                   window.location.href = "3.htm";//跳转目标
                } else {
                    alert("请认真填写帐号密码");                    
                }
            }*/
        });
});
});
function callBack(data) {
	if (data.type == "alert") {
		alert(data.message);
		//jQuery("#padder1").show();
